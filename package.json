{
  "name": "vscode-npm-lens",
  "displayName": "npmLens",
  "version": "2.0.1",
  "description": "Visual panel for project dependencies: Monitor updates, analyze size, and more.",
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "npm",
    "dependencies",
    "package",
    "yarn",
    "pnpm",
    "package.json",
    "update",
    "upgrade",
    "bundlephobia",
    "size"
  ],
  "bugs": {
    "email": "dev@pilaton.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pilaton/vscode-npm-lens-support.git"
  },
  "license": "MIT",
  "author": {
    "name": "Pilaton",
    "email": "dev@pilaton.com",
    "url": "https://github.com/Pilaton"
  },
  "publisher": "Pilaton",
  "main": "./dist/extension.js",
  "scripts": {
    "build:all": "pnpm run clean && pnpm run build:tree & pnpm run build:web",
    "build:tree": "vite build -c vite.tree.config.mts",
    "build:web": "vite build -c vite.web.config.mts",
    "clean": "rimraf ./dist",
    "dev:all": "pnpm run clean && pnpm run dev:tree & pnpm run dev:web",
    "dev:tree": "vite build --watch --mode=development -c vite.tree.config.mts",
    "dev:ts": "tsc --watch --outDir dist",
    "dev:web": "vite build --watch --mode=development -c vite.web.config.mts",
    "knip": "knip",
    "lint": "eslint src --ext ts,tsx",
    "prepackage": "pnpm run build:all",
    "package": "vsce package --no-dependencies --out ./vsix",
    "postpackage": "pnpm run clean",
    "release": "standard-version --skip.commit --skip.tag",
    "release-demo": "standard-version --dry-run --skip.commit --skip.tag"
  },
  "contributes": {
    "views": {
      "npm-lens": [
        {
          "id": "npm-lens.treeView",
          "name": "Hello!",
          "type": "tree"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "npm-lens",
          "title": "npmLens",
          "icon": "public/icon-panel.svg"
        }
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "pretty-bytes": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/eslint": "^8.44.7",
    "@types/node": "20.9.4",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/semver": "^7.5.6",
    "@types/vscode": "1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "knip": "^2.42.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "terser": "^5.24.0",
    "ts-deepmerge": "^6.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "public/marketplace-icon-384px.png",
  "extensionKind": [
    "workspace"
  ],
  "pricing": "Free"
}
