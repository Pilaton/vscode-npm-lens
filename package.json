{
  "name": "vscode-npm-lens",
  "displayName": "npmLens",
  "version": "1.1.4",
  "description": "Visual panel for project dependencies: Monitor updates, analyze size, and more.",
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "npm",
    "dependencies",
    "package",
    "yarn",
    "pnpm",
    "package.json",
    "update",
    "upgrade",
    "bundlephobia",
    "size"
  ],
  "bugs": {
    "email": "dev@pilaton.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pilaton/vscode-npm-lens-support.git"
  },
  "license": "MIT",
  "author": {
    "name": "Pilaton",
    "email": "dev@pilaton.com",
    "url": "https://github.com/Pilaton"
  },
  "publisher": "Pilaton",
  "main": "./dist/extension.js",
  "scripts": {
    "build:webpack": "webpack --config webpack.prod.js",
    "clean": "rimraf ./dist",
    "dev:ts": "tsc --watch --outDir dist",
    "dev:webpack": "webpack --watch --config webpack.dev.js",
    "lint": "eslint src --ext ts,tsx",
    "prepackage": "npm run clean && npm run build:webpack",
    "package": "vsce package --no-dependencies --out ./vsix",
    "postpackage": "npm run clean",
    "release": "standard-version --skip.commit --skip.tag",
    "release-demo": "standard-version --dry-run --skip.commit --skip.tag"
  },
  "contributes": {
    "views": {
      "npm-lens": [
        {
          "id": "npm-lens.treeView",
          "name": "Hello!",
          "type": "tree"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "npm-lens",
          "title": "npmLens",
          "icon": "public/icon-panel.svg"
        }
      ]
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "pretty-bytes": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@svgr/webpack": "^8.0.1",
    "@types/node": "20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/semver": "^7.5.0",
    "@types/vscode": "^1.79.0",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-loader": "^9.1.2",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "icon": "public/marketplace-icon-384px.png",
  "extensionKind": [
    "workspace"
  ],
  "pricing": "Free"
}
